  <div style="padding:25px;padding-top:0px;">

	<div class="row">
	  
	  <div class="col-md-0"></div>
	  
	  <div class="col-md-11">
	  	
		   <h4>Sign In</h4>

		   <p>
		   	<%=flash[:notice]%>
		   </p>

		  <form role="form" name="user" id = "user" action="/sessions" method="post">
		  
		  <div class="form-group">
		    <label for="name">Email Address</label>
		    <input type="email" class="form-control" name = "email" id="email" placeholder="Email Address">
		  </div>

		  <div class="form-group">
		    <label for="name">Password</label>
		    <input type="password" class="form-control" name = "password" id="password" placeholder="Password">
		  </div>


		  <button type="submit" class="btn btn-success">Submit</button>
		  <span style = "margin-left:15px;"><a href="/applications">Cancel</a></span>
		 </form>

	  </div>

	  <br>
	  <h4>Don't have an account? <a href = "/users/signup">Sign Up.</a></h4>

	  <div class="col-md-1"></div>
	
	</div>

</div> 